    <script>
      $(document).ready(async () => {
        let params = new URL(document.location).searchParams;
        var a = params.get('ccode');
        var b = params.get('icode');
        await getCategory();
        await getInfo(a, b);
      });
      getInfo = async (a, b) => {
        try {
          let response = await fetch('https://mostfeel.site:8888/admin/detail/info', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json'
            },
            body: JSON.stringify({ cmp_seq: a, items_seq: b })
          });
          let json = await response.json();
          console.log(json)
          if (response.ok) {
            setCompanyProfile(json.cmp[0]);
            setRelated(json.related);
            setItemInfo();
          }
        } catch (err) {
          console.log(err);
        }
      }
      let category = new Array();
      getCategory = async (a, b) => {
        try {
          let response = await fetch('https://mostfeel.site:8888/admin/category/categories', {
            method: 'GET',
            headers: {
              'Content-Type': 'application/json'
            },
          });
          let json = await response.json();
          if (response.ok) {
            console.log('categories : ', json)
            category = json;
          }
        } catch (err) {
          console.log(err);
        }
      }
      setCompanyProfile = (data) => {
        var div = document.getElementsByClassName('contact-seller')[0];
        var img = document.createElement('img');
        img.src = data.profile;
        div.appendChild(img);
        var cmpName = document.getElementById('cmp-name');
        var cmpLocation = document.getElementById('cmp-location');
        var cmpDate = document.getElementById('cmp-reg-date');
        var cmpCategory = document.getElementById('cmp-category');
        var cmpPhone = document.getElementById('cmp-phone');
        cmpName.innerHTML = '<i class="fas fa-building"></i> 업체 명 :' + data.cmp_name;
        cmpLocation.innerHTML = '<i class="fa fa-map-marker" aria-hidden="true"></i> 업체위치 : ' + data.cmp_location;
        cmpDate.innerHTML = '<i class="fas fa-microphone"></i> 가입일자 : ' + data.reg_date.substring(0, 10);
        var a = category.filter(item => item.category_seq == data.category_seq);
        cmpCategory.innerHTML = '<i class="fas fa-microphone"></i> 카테고리 : ' + a[0].category_name;
        cmpPhone.innerHTML = '<i class="fa fa-mobile" aria-hidden="true"></i>' + data.cmp_phone
      }

      setRelated = (data) => {
        var rawArray = data;
        var divList = document.getElementById('related-item');
        for (var i = 0; i < rawArray.length; i++) {
          var status;
          if(rawArray[i].cancelled == 0) status = '게시중';
          if(rawArray[i].cancelled == 1) status = '삭제';
          var newDiv = document.createElement('div');
          newDiv.setAttribute("class", "col-lg-3 col-md-6 col-sm-6 col-xs-12 col-12 Classidied-box");
          newDiv.innerHTML =
            `<div class="card card-event info-overlay">
              <div class="img has-background" style="background-image: url(`+ rawArray[i].image[0].uri + `); background-size:cover ">
                <div> <img alt="340x230" class="card-img-top img-responsive"
                  data-holder-rendered="true" src="` + rawArray[i].image[0].uri + `"> 
                </div>
              </div>
              <div class="card-body">
                <h4 class="card-title"> <a href="addetail.html">` + rawArray[i].item_name + `</a> </h4>
                <div class="card-event-info">
                  <p class="event-location"><i class="fas fa-check-circle"></i> 광고여부 : ` + status + `</p>
                  <p class="event-time"><i class="far fa-clock"></i> 등록일자 : ` + rawArray[i].reg_date.substring(0, 10) + `</p>
                </div>
              </div>
              <div class="card-footer">
                <div class="pull-left left">
                  <div class=""><a href="https://mostfeel.site:8888/admin/detail/item?ccode=` + rawArray[i].cmp_seq +"&icode="+ rawArray[i].items_seq+`">업체로 이동</div>
                </div>
              </div>
            </div>`;
          divList.appendChild(newDiv);
        }
      }
    </script>
