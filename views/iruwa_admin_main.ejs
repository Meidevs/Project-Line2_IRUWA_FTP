  <!-- Optional JavaScript -->
  <!-- jQuery first, then Popper.js, then Bootstrap JS -->
  <script src="https://mostfeel.site:8888/static//javascripts/js/jquery.min.js" type="text/javascript"></script>
  <script src="https://mostfeel.site:8888/static//javascripts/js/popper.min.js" type="text/javascript"></script>
  <script src="https://mostfeel.site:8888/static//javascripts/js/bootstrap.min.js" type="text/javascript"></script>
  <script src="https://mostfeel.site:8888/static//javascripts/js/functions.js" type="text/javascript"></script>
  <script src="https://mostfeel.site:8888/static//javascripts/js/owl.carousel.min.js" type="text/javascript"></script>
  <script src="https://mostfeel.site:8888/static//javascripts/js/slick.js" type="text/javascript"></script>
  <script src="https://mostfeel.site:8888/static//javascripts/js/swiper.min.js" type="text/javascript"></script>
  <script src="https://mostfeel.site:8888/static//javascripts/js/main.js" type="text/javascript"></script>
  <script>
    $(document).ready(async () => {
      getPopularCompany();
      getRecentCompany();
    });
    getPopularCompany = async () => {
      try {
        let response = await fetch('https://mostfeel.site:8888/admin/popular', {
          method: 'GET',
          headers: {
            'Content-Type': 'application/json'
          }
        });
        let json = await response.json();
        if (response.ok) {
          var rawArray = json.message;
          var divList = document.getElementById('popular-item');
          for (var i = 0; i < rawArray.length; i++) {
            var newDiv = document.createElement('div');
            newDiv.setAttribute("class", "col-lg-4 col-md-6 col-sm-6 col-xs-12 col-12 Classidied-box");
            newDiv.innerHTML =
              `<div class="card card-event info-overlay">
              <div class="img has-background" style="background-image: url(`+ rawArray[i].image[0] + `); background-size:cover ">
                <div class="event-pop-link"> 
                  <span class="event-badges "></span>
                  <div class="event-pop-info">
                    <p class="publisher"><strong>` + rawArray[i].item_name + `</strong></p>
                    <div class="event-rating">
                      <div class="review-count"> 좋아요 | 89 조회수</div>
                    </div>
                  </div>
                </div> 
                <div> <img alt="340x230" class="card-img-top img-responsive"
                  data-holder-rendered="true" src="` + rawArray[i].image[0] + `"> 
                </div>
              </div>
              <div class="card-body">
                <h4 class="card-title"> <a href="addetail.html">` + rawArray[i].item_name + `</a> </h4>
                <div class="card-event-info">
                  <p class="event-location"><i class="far fa-building"></i><a class="location" href=""> 업체 명 : ` + rawArray[i].cmp.cmp_name + `</a></p>
                  <p class="event-location"><i class="fas fa-compass"></i><a class="location" href=""> 업체위치 : ` + rawArray[i].cmp.cmp_detail_location + `</a></p>
                  <p class="event-time"><i class="far fa-clock"></i> 등록일자 : ` + rawArray[i].reg_date.substring(0, 10) + `</p>
                </div>
              </div>
              <div class="card-footer">
                <div class="pull-left left">
                  <div class=""><a href="https://mostfeel.site:8888/admin/detail/?ccode=` + rawArray[i].cmp.cmp_seq + `">업체로 이동</div>
                </div>
              </div>
            </div>`;
            divList.appendChild(newDiv);
          }
        }
      } catch (err) {
        console.log(err);
      }
    }

    getRecentCompany = async () => {
      try {
        let response = await fetch('https://mostfeel.site:8888/admin/recentcompany', {
          method: 'GET',
          headers: {
            'Content-Type': 'application/json'
          }
        });
        let json = await response.json();
        if (response.ok) {
          var rawArray = json.message;
          var divList = document.getElementById('new-company');
          for (var i = 0; i < rawArray.length; i++) {
            var newDiv = document.createElement('div');
            newDiv.setAttribute("class", "col-lg-4 col-md-6 col-sm-6 col-xs-12 col-12 Classidied-box");
            newDiv.innerHTML =
              `<div class="card card-event info-overlay">
            <div class="img has-background"
              style="background-image: url(` + rawArray[i].profile + `); background-size:cover ">
              <div class="event-pop-link"> 
                  <span class="event-badges "></span>
                  <div class="event-pop-info">
                    <p class="publisher"><strong>` + rawArray[i].cmp_name + `</strong></p>
                  </div>
                </div> 
                <div> <img alt="340x230" class="card-img-top img-responsive"
                  data-holder-rendered="true" src="` + rawArray[i].profile + `"> 
                </div> 
            </div>
            <div class="card-body">
              <h4 class="card-title"> <a href="addetail.html">` + rawArray[i].cmp_name + `</a> </h4>
              <div class="card-event-info">
                <p class="event-location"><i class="far fa-compass"></i> <a class="location" href="">` + rawArray[i].cmp_detail_location + ` </a></p>
                <p class="event-time"><i class="far fa-clock"></i> ` + rawArray[i].reg_date.substring(0,10) + `</p>
              </div>
            </div>
            <div class="card-footer">
              <div class="pull-left left">
                <div class=""><a href="https://mostfeel.site:8888/admin/detail/?ccode=` + rawArray[i].cmp_seq + `">업체로 이동</div>
              </div>
            </div>
          </div>`;
            divList.appendChild(newDiv);
          }
        }
      } catch (err) {
        console.log(err);
      }
    }
  </script>
